$(function(){
  $('#submit').click(whatup);
});

function whatup () {

  url = $('#url').val();
  depth = $('#depth').val();
  $.ajax({
  type: "POST",
  url: "/data",
  data: { url: url, depth: depth }
  }).done(function( msg ) {
    $('#images').empty();
    for (var i=0; i<msg.length; i++)
    {
      url = msg[i].url
      img = $('<img>')
      img.attr('src', url)
      $('#images').append(img);
    }
});

}